  "files": [
    "plugin"
  ],
  "main": "plugin/styleguide-plugin.js",
  "directories": {
    "lib": "plugin",
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pure-ui/styleguide"
  },
  "engines": {
    "node": "5.5.0"
  },
  "scripts": {
    "start": "npm run example:dev",
    "preserver": "npm run client:build",
    "server": "webpack-dev-server",
    "example:dev": "npm run server -- --context examples/dev/ --config examples/dev/webpack.dev.babel.js",
    "example:dev:iron": "iron-node node_modules/webpack-dev-server/bin/webpack-dev-server.js --context examples/dev/ --config examples/dev/webpack.dev.babel.js",
    "example:dest": "npm run server -- --context examples/dest/ --config examples/dest/webpack.dev.babel.js",
    "example:dest:iron": "iron-node node_modules/webpack-dev-server/bin/webpack-dev-server.js --context examples/dest/ --config examples/dest/webpack.dev.babel.js",
    "client:dev": "webpack-dev-server --hot --config client/webpack.dev.babel.js",
    "client:build": "webpack --config client/webpack.prod.babel.js --progress -p",
    "build": "npm run client:build",
    "lint": "npm run lint:eslint  && npm run lint:jscs",
    "lint:eslint": "eslint ./",
    "lint:jscs": "jscs ./",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register --require testHelper.js client/**/__test__/*.js plugin/**/__test__/*.js",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "babel-standalone": "^6.4.4",
    "connect-history-api-fallback": "^1.1.0",
    "minimatch": "^3.0.0",
    "mocha": "^2.4.5",
    "react": "=0.14.4",
    "react-dom": "=0.14.4",
    "react-router": "2.0.0-rc5"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "doctrine": "^1.1.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.15.0",
    "express": "^4.13.4",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.7.6",
    "iron-node": "^2.1.13",
    "jscs": "^2.9.0",
    "json-loader": "0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "proxy-middleware": "^0.15.0",
    "react-docgen": "2.7.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webdriverio": "^3.4.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-merge": "0.7.3"
  }
}
